factories:

  lightGalleryControl:
    create: \Flame\Components\LightGallery\LightGalleryControl(%images%)
    implement: \Flame\Components\LightGallery\ILightGalleryControlFactory
    parameters: [images]

  imageForm:
    create: \Flame\CMS\AdminModule\Forms\Images\ImageForm(%categories%, %defaults%)
    implement: \Flame\CMS\AdminModule\Forms\Images\IImageFormFactory
    parameters: [array categories, array defaults = []]


